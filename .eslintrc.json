{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "consistent-return": "off",
        "react/prop-types": "off",
        "react/jsx-props-no-spreading": "off",
        "unicorn/filename-case": "off",
        "unicorn/no-reduce": "off",
        "unicorn/no-useless-undefined": "off"
    }
}
